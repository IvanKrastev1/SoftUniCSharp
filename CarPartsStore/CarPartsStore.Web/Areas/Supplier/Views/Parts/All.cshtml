@model IEnumerable<AllPartsListingModel>
@{
    ViewData["Title"] = "All";
}


<h3 style="color:lightgreen">@TempData["SuccessDelete"]</h3>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Price</th>
            <th scope="col">In Stock</th>
            <th scope="col">Car</th>
        </tr>
    </thead>

    @foreach (var part in Model)
    {
        <tbody>
            <tr class="table-active">
                <td>@part.Name</td>
                <td>@part.Price</td>
                <td>@part.Quantity</td>
                <td>@part.Car.Make @part.Car.Model @part.Car.Year @part.Car.Fuel @part.Car.Motor </td>
            </tr>
        </tbody>
    }
</table>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
